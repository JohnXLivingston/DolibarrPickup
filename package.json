    "@types/nunjucks": "^3.1.3",
    "@types/select2": "^4.0.49",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
