<div class="alert alert-info">
  {{ translate('Please confirm the data you want to save:') }}
</div>
<table class="table table-striped table-bordered">
  <tbody>
  {% for sv in stackValues %}
    {% set s = getDisplayStackValue(sv) %}
    {% if s %}
      <tr>
        <th>{{ s.label }}</th>
        <td>{{ s.display }}</td>
      </tr>
    {% endif %}
  {% endfor %}
  </tbody>
</table>

<hr>
<nav>
  <input type="submit" value="{{ translate('Save') }}" class="btn btn-primary"/>
</nav>
<div class="alert alert-danger d-none" pickupmobile-save-error-container></div>
